<div class="login-container">
  <h2>Đăng nhập</h2>
  <%= form_with url: "/login", method: :post, data: { turbo: false } do |f| %>
    <div class="form-group">
      <%= f.label :account, "Email hoac so dien thoai" %>
      <%= f.text_field :account %>
      

    </div>
    <div class="form-group">
      <%= f.label :password %>
      <%= f.text_field :password, type: "password" %>
    </div>
    <button type="submit">Đăng nhập</button>
  <% end %>
</div>